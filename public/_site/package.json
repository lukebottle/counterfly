    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.14",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.2.4",
    "tailwindcss-bg-patterns": "^0.2.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.8",
    "autoprefixer": "^10.4.1",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "cssnano": "^5.0.14",
    "express": "^4.18.2",
    "github-pages": "^3.0.2",
    "html-minifier": "^4.0.0",
    "node-fetch": "^3.3.0",
    "postcss-cli": "^9.1.0",
    "ruby": "^0.6.1",
    "tailwindcss": "^3.0.11"
  }
